<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>Ram App</name>

  <description>
      An example for phonegap build docs.
  </description>

  <author href="https://build.phonegap.com" email="support@phonegap.com">
      Hardeep Shoker
  </author>
  
  <!-- Add the whitelist plugin -->
<plugin name="cordova-plugin-whitelist" spec="*"/>


 
  <access origin="*" />
  <allow-navigation href="*" />
<allow-intent href="*://*/*" />
  
  
  <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="Cache" >
                <param name="android-package" value="com.phonegap.example.Cache"/>
            </feature>
        </config-file>
    </platform>
  
  <uses-permission android:name="com.phonegap.example.permission.READ_EXTERNAL_STORAGE"/>
  <uses-permission android:name="com.phonegap.example.permission.WRITE_EXTERNAL_STORAGE"/>

</widget>